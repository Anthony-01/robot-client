.wrapper
  position: relative;
  width 100%;
  height 100%;
  min-width 600px;
  .header
    height: 60px;
    background: #1e9fff;
    .logo
      position: absolute;
      //margin 0 auto;
      left 20px;
      top: 7px
    .info
      position: absolute
      right 50px;
      height 100%;
      .user
        display inline-block
        //height 100%;
        padding 17px 50px;
        line-height: 16px;
        .avatar
          width: 32px;
          height 32px;
          border-radius 16px;
          vertical-align middle
        .username
          font-size: 14px;
          vertical-align middle
          color: #fff
      .btn-exit
        display inline-block
        padding 17px 0;
        cursor: pointer;
        .text
          line-height: 16px;
          font-size: 16px;
          vertical-align: middle
          color: #fff
        .icon
          vertical-align: middle
          color: #fff
  .main-wrapper
    display flex;
    .aside
      width 200px;
      height 100%;
    .content-wrapper
      flex-grow: 1;
      background: #eef1f5;
      padding: 10px 30px 15px 10px;
      .show-view
        position relative
        box-sizing border-box
        height: 100%;
        background: #ffffff;
        overflow hidden;


