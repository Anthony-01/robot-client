.main_control_wrapper
  position relative
  padding 20px 15px 20px;
  background: #ffffff;
  min-width 600px;
  .header
    display: flex;
    flex-wrap: nowrap;
    justify-content: space-between;
    padding-bottom 20px;
    height 41px;
    .txt
      display: flex;
      align-items: flex-end;
      width: 580px;
      .txt
        display inline-block;
        font-size: 20px;
        line-height 20px;
      .total-txt
        color: #fc9409;
      .had-txt
        color: #83d45d
      .have-not-txt
        color: #3dacff
      .txt-item
        height: 20px;
    .buttons
      width 700px;
      display: flex;
      justify-content: space-between;
      //align-items flex-end;

  .show-list
    .main-table
      border-collapse: collapse;
      border-spacing: 0;
      //table-layout: fixed
      text-align: center;
      table-layout: automatic
      .table-title
        height: 40px;
        color: #7e90a3
        border-top 2px solid #1e9fff;
        border-left 2px solid #e2e6ec;
        border-right 2px solid #e2e6ec;
        background: #f4f6f9;
        width: 100%;
        .table-title-item
          line-height 40px;
          border-right 2px solid #e2e6ec;
          text-align: center;
      .table-row
        display flex;
        border-bottom 2px solid #e2e6ec;
        justify-content space-between
        line-height 50px;
      .table-body
        padding 0;
        .server-time
          color: #4db0ff;
          font-size: 16px;
        .check-input
          color: blue;
        .table-item-operate
          display: flex;
          justify-content flex-end;
          align-items center;
          line-height: 0px;
          .table-btn-wrapper
            display: flex;

            justify-content flex-end;
            height: 36px;
            .table-btn
              margin-left: 8px;
  .btn-wrapper
    display: flex;
    margin-top 50px;
    align-items flex-end;
    justify-content space-between;
    .btn-all
      display: flex;
      align-items center;
      margin-left 28px;
    .check-input
      height 16px;
    .txt
      margin-left 8px;
  .nav-footer
    margin-top: 20px;
  .footer
    position absolute;
    font-size: 12px;
    right 10px;
    bottom: 10px;
    color: #7e90a3;

